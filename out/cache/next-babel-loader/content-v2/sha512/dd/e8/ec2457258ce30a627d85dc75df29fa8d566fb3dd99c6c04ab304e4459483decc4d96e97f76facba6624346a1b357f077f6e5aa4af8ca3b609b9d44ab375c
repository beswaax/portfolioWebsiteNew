ize: \"28px\",\n      lineHeight: \"36px\",\n    },\n    [theme.breakpoints.down(\"xs\")]: {\n      fontSize: \"30px\",\n      lineHeight: \"30px\",\n    },\n    [theme.breakpoints.down(theme.breakpoints.values.verySmall)]: {\n      fontSize: \"25px\",\n    },\n  },\n  scheduleText: {\n    color: \"#FFFFFF\",\n    fontSize: \"15px\",\n    fontWeight: 1200,\n    fontFamily: \"Ubuntu, sans-serif\",\n    padding: 8,\n    [theme.breakpoints.down(\"sm\")]: {\n      fontSize: \"14px\",\n    },\n    [theme.breakpoints.down(\"xs\")]: {\n      fontSize: \"14px\",\n    },\n    [theme.breakpoints.down(theme.breakpoints.values.xsSmaller)]: {\n      fontSize: \"12px\",\n    },\n    [theme.breakpoints.down(theme.breakpoints.values.verySmall)]: {\n      fontSize: \"10px\",\n    },\n  },\n\n  textfield: {\n    marginBottom: \"1rem\",\n    width: \"100%\",\n  },\n}));\n\nconst CallToAction = () => {\n  const classes = useStyles();\n  const theme = useTheme();\n\n  const [dialogOpen, setDialogOpen] = useState(false);\n  const [firstName, setFirstName] = useState(\"\");\n  const [lastName, setLastName] = useState(\"\");\n  const [email, setEmail] = useState(\"\");\n  const [phone, setPhone] = useState(\"\");\n  const [emailError, setEmailError] = useState(false);\n  const [loading, setLoading] = useState(false);\n  const [alert, setAlert] = useState({\n    open: false,\n    message: \"\",\n    backgroundColor: \"\",\n  });\n\n  const matchesSM = useMediaQuery(theme.breakpoints.down(\"sm\"));\n  const matchesXS = useMediaQuery(theme.breakpoints.down(\"xs\"));\n\n  const onConfirm = () => {\n    //setLoading to true, so that when loading is true it will display a loading spinner instead of the text, you have to change that in the Button\n\n    //after the request setLoading to false and show a snackbar\n\n    //show error snackbar when there is an error.\n    //show success snackbar after no error\n\n    setLoading(true);\n    Axios.get(\n      \"https://us-central1-portfolio-site-c033a.cloudfunctions.net/sendConsultation\",\n      {\n        params: {\n          firstName: firstName,\n          lastName: lastName,\n          email: email,\n          phone: phone,\n        },\n      }\n    )\n      .then((res) => {\n        setLoading(false);\n        setEmailError(false);\n        setFirstName(\"\");\n        setLastName(\"\");\n        setEmail(\"\");\n        setPhone(\"\");\n        setDialogOpen(false);\n        setAlert({\n          open: true,\n          message: \"Message sent successfully\",\n          backgroundColor: \"#4BB543\",\n        });\n      })\n      .catch((err) => {\n        setLoading(false);\n        setEmailError(false);\n        setFirstName(\"\");\n        setLastName(\"\");\n        setEmail(\"\");\n        setPhone(\"\");\n        setDialogOpen(false);\n        setAlert({\n          open: true,\n          message: \"Something went wrong, please try again!\",\n          backgroundColor: \"#FF3232\",\n        });\n      });\n  };\n\n  return (\n    <Grid\n      item\n      container\n      direction=\"column\"\n      justify=\"center\"\n      alignItems=\"center\"\n      className={classes.backgroundImageContainer}\n    >\n      <Grid\n        item\n        style={{\n          marginTop: matchesXS ? \"5rem\" : matchesSM ? \"12rem\" : \"15rem\",\n          padding: 15,\n        }}\n      >\n        <Typography variant=\"h3\" align=\"center\" className={classes.titleText}>\n          Affordable Web Development & Web Design solutions\n        </Typography>\n        <Typography\n          variant=\"h1\"\n          style={{ marginTop: \"1.5rem\", marginBottom: \"3rem\" }}\n          align=\"center\"\n          className={classes.bigText}\n        >\n          WE HELP YOU GROW YOUR <br />\n          BUSINESS\n        </Typography>\n      </Grid>\n\n      <Grid\n        item\n        style={{\n          marginBottom: matchesXS ? \"8rem\" : matchesSM ? \"12rem\" : \"15rem\",\n        }}\n      >\n        <Button\n          style={{ borderRadius: 0, width: \"100%\" }}\n          align=\"center\"\n          disableRipple\n          variant=\"contained\"\n          color=\"primary\"\n          onClick={() => setDialogOpen(!dialogOpen)}\n        >\n          <Typography align=\"center\" className={classes.scheduleText}>\n            Book a free consultation\n          </Typography>\n        </Button>\n      </Grid>\n\n      <Dialog\n        open={dialogOpen}\n        onClose={() => {\n          setFirstName(\"\");\n          setLastName(\"\");\n          setEmail(\"\");\n          setPhone(\"\");\n          setDialogOpen(!dialogOpen);\n        }}\n        style={{ borderRadius: 0, marginTop: matchesXS ? \"3.4rem\" : 0 }}\n      >\n        <Grid container direction=\"column\" justify=\"center\" alignItems=\"center\">\n          <DialogTitle align=\"center\" id=\"simple-dialog-title\">\n            BOOK A CONSULTATION\n          </DialogTitle>\n          <Grid\n            item\n            container\n            direction=\"column\"\n            alignItems=\"center\"\n            justify=\"center\"\n            style={{ padding: 10, width: \"100%\", maxWidth: \"40rem\" }}\n          >\n            <Grid item align=\"center\" style={{ width: \"100%\" }}>\n              <TextField\n                label=\"First Name\"\n                color=\"secondary\"\n                value={firstName}\n                variant=\"outlined\"\n                onChange={(e) => setFirstName(e.target.value)}\n                className={classes.textfield}\n              />\n            </Grid>\n            <Grid item align=\"center\" style={{ width: \"100%\" }}>\n              <TextField\n                label=\"Last Name\"\n                color=\"secondary\"\n                value={lastName}\n                onChange={(e) => setLastName(e.target.value)}\n                variant=\"outlined\"\n                className={classes.textfield}\n              />\n            </Grid>\n\n            <Grid item align=\"center\" style={{ width: \"100%\" }}>\n              <TextField\n                label=\"Email\"\n                color=\"secondary\"\n                error={emailError}\n                value={email}\n                onChange={(e) => {\n                  setEmail(e.target.value);\n                }}\n                onChange={(e) => setEmail(e.target.value)}\n                variant=\"outlined\"\n                className={classes.textfield}\n              />\n            </Grid>\n            <Grid item align=\"center\" style={{ width: \"100%\" }}>\n              <TextField\n                label=\"Phone\"\n                color=\"secondary\"\n                type=\"number\"\n                value={phone}\n                onChange={(e) => setPhone(e.target.value)}\n                variant=\"outlined\"\n                className={classes.textfield}\n              />\n            </Grid>\n          </Grid>\n\n          <Grid\n            item\n            container\n            direction={\"row\"}\n            justify=\"center\"\n            alignItems=\"center\"\n            style={{ marginBottom: \"1rem\" }}\n          >\n            <Button\n              style={{ marginRight: \"1em\" }}\n              variant=\"contained\"\n              disableRipple\n              onClick={() => {\n                setFirstName(\"\");\n                setLastName(\"\");\n                setEmail(\"\");\n                setPhone(\"\");\n                setDialogOpen(!dialogOpen);\n              }}\n              color=\"secondary\"\n            >\n              Cancel\n            </Button>\n            {loading ? (\n              \"Sending...\"\n            ) : (\n              <Button\n                variant=\"contained\"\n                disableRipple\n                onClick={() => onConfirm()}\n                disabled={\n                  firstName.length < 1 ||\n                  lastName.length < 1 ||\n                  email.length < 1 ||\n                  phone.length < 4 ||\n                  !email.includes(\"@\")\n                }\n                color=\"primary\"\n              >\n                Send Message\n              </Button>\n            )}\n          </Grid>\n        </Grid>\n      </Dialog>\n      <Snackbar\n        anchorOrigin={{ vertical: \"bottom\", horizontal: \"center\" }}\n        open={alert.open}\n        autoHideDuration={6000}\n        ContentProps={{\n          style: { backgroundColor: alert.backgroundColor },\n        }}\n        message={alert.message}\n        onClose={() =>\n          setAlert({ open: false, message: \"\", backgroundColor: \"\" })\n        }\n      />\n    </Grid>\n  );\n};\n\nexport default CallToAction;\n"]},"metadata":{},"sourceType":"module"}