 },\n  listItem: {\n    backgroundColor: \"#ffffff\",\n  },\n}));\n\nexport default function Navigation({ tabValue, setTabValue }) {\n  const classes = useStyles();\n  const router = useRouter();\n\n  const content = {\n    brand: { image: \"/assets/vector/default-monochrome.svg\", width: 220 },\n    link1: \"Home\",\n    link2: \"About\",\n    link3: \"Services\",\n    link4: \"Contact Us\",\n  };\n\n  let brand;\n\n  if (content.brand.image) {\n    brand = (\n      <img src={content.brand.image} alt=\"\" width={content.brand.width} />\n    );\n  } else {\n    brand = content.brand.text || \"\";\n  }\n\n  const [state, setState] = React.useState({ open: false });\n\n  const toggleDrawer = (open) => (event) => {\n    if (\n      event.type === \"keydown\" &&\n      (event.key === \"Tab\" || event.key === \"Shift\")\n    ) {\n      return;\n    }\n\n    setState({ ...state, open });\n  };\n\n  useEffect(() => {\n    if (router.pathname === \"/\" && tabValue !== 0) {\n      setTabValue(0);\n    } else if (router.pathname === \"/about\" && tabValue !== 1) {\n      setTabValue(1);\n    } else if (router.pathname === \"/services\" && tabValue !== 2) {\n      setTabValue(2);\n    } else if (router.pathname === \"/contact\" && tabValue !== 3) {\n      setTabValue(3);\n    }\n  }, [setTabValue, tabValue, router.pathname]);\n\n  return (\n    <AppBar position=\"static\" color=\"inherit\" className={classes.appBar}>\n      <Toolbar className={classes.toolbar}>\n        <Link\n          href=\"/\"\n          color=\"primary\"\n          underline=\"none\"\n          variant=\"h5\"\n          className={classes.brand}\n        >\n          {brand}\n        </Link>\n        <Link\n          href=\"/\"\n          color=\"textPrimary\"\n          variant=\"body2\"\n          className={tabValue === 0 ? classes.linkSelected : classes.link}\n        >\n          {content[\"link1\"]}\n        </Link>\n        <Link\n          href=\"/about\"\n          color=\"textPrimary\"\n          variant=\"body2\"\n          className={tabValue === 1 ? classes.linkSelected : classes.link}\n        >\n          {content[\"link2\"]}\n        </Link>\n        <Link\n          href=\"/services\"\n          color=\"textPrimary\"\n          variant=\"body2\"\n          className={tabValue === 2 ? classes.linkSelected : classes.link}\n        >\n          {content[\"link3\"]}\n        </Link>\n        <Link\n          href=\"/contact\"\n          color=\"textPrimary\"\n          variant=\"body2\"\n          className={tabValue === 3 ? classes.linkSelected : classes.link}\n        >\n          {content[\"link4\"]}\n        </Link>\n        <IconButton\n          edge=\"start\"\n          color=\"inherit\"\n          aria-label=\"menu\"\n          className={classes.menuButton}\n          onClick={toggleDrawer(!state.open)}\n        >\n          <MenuIcon />\n        </IconButton>\n      </Toolbar>\n      <Drawer anchor=\"left\" open={state.open} onClose={toggleDrawer(false)}>\n        <div className={classes.drawerContainer}>\n          <Box\n            mb={1}\n            ml={2}\n            pb={2}\n            border={1}\n            borderTop={0}\n            borderLeft={0}\n            borderRight={0}\n            borderColor=\"#000000\"\n          >\n            <Link\n              href=\"/\"\n              color=\"primary\"\n              underline=\"none\"\n              variant=\"h5\"\n              className={classes.linkBrand}\n            >\n              {brand}\n            </Link>\n          </Box>\n          <List>\n            <ListItem\n              className={classes.listItem}\n              button\n              key={content[\"link1\"]}\n              selected={tabValue === 0}\n              component={Link}\n              href=\"/\"\n              onClick={() => setState({ open: !state.open })}\n            >\n              <ListItemText primary={content[\"link1\"]} />\n            </ListItem>\n            <ListItem\n              button\n              key={content[\"link2\"]}\n              selected={tabValue === 1}\n              component={Link}\n              href=\"/about\"\n              onClick={() => setState({ open: !state.open })}\n            >\n              <ListItemText primary={content[\"link2\"]} />\n            </ListItem>\n            <ListItem\n              button\n              key={content[\"link3\"]}\n              selected={tabValue === 2}\n              component={Link}\n              href=\"/services\"\n              onClick={() => setState({ open: !state.open })}\n            >\n              <ListItemText primary={content[\"link3\"]} />\n            </ListItem>\n            <ListItem\n              button\n              key={content[\"link4\"]}\n              selected={tabValue === 3}\n              component={Link}\n              href=\"/contact\"\n              onClick={() => setState({ open: !state.open })}\n            >\n              <ListItemText primary={content[\"link4\"]} />\n            </ListItem>\n          </List>\n        </div>\n      </Drawer>\n    </AppBar>\n  );\n}\n"]},"metadata":{},"sourceType":"module"}